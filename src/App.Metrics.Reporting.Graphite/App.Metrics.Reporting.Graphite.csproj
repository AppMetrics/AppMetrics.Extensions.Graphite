<Project Sdk="Microsoft.NET.Sdk">

	<Import Project="..\..\build\common.props" />

	<PropertyGroup>
		<Description>Provides Graphite reporting capability to App Metrics</Description>        
		<TargetFrameworks>netstandard1.6</TargetFrameworks>
		<PackageTags>appmetrics;reporting;graphite</PackageTags>    
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\App.Metrics.Formatters.Graphite\App.Metrics.Formatters.Graphite.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="App.Metrics.Abstractions" Version="$(AppMetricsCoreVersion)" />
	</ItemGroup>

</Project>
